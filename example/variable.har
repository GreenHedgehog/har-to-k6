{
    "log": {
        "entries": [
            {
                "request": {
                    "url": "http://api.example.com/authenticate",
                    "method": "POST",
                    "postData": {
                        "mimeType": "application/json",
                        "text": "{\"username\":\"admin\",\"password\":\"123\"}"
                    }
                },
                "variables": [
                    {
                        "name": "accessToken",
                        "type": 0,
                        "expression": "user.token"
                    }
                ]
            },
            {
              "request": {
                "method": "POST",
                "url": "https://httpbin.org/post",
                "comment": "Post about found images",
                "headers": [
                    {
                        "name": "Authorization",
                        "value": "Bearer ${accessToken}"
                    }
                ],
                "postData": {
                    "mimeType": "application/x-www-form-urlencoded",
                    "params": [
                        {
                            "name": "q",
                            "value": "hello${accessToken}"
                        },
                        {
                            "name": "utm",
                            "value": "a1"
                        },
                        {
                            "name": "utm",
                            "value": "b2"
                        }
                    ]
                }
              }
            }
        ]
    }
}
